{
  "name": "happa",
  "components": {
    "nginx": {
      "image": "registry.giantswarm.io/giantswarm/happa:0.1.1",
      "ports": 80,
      "domains": {
        "80": "happa.giantswarm.io"
      },
      "scale": {
        "min": 3,
        "placement": "one-per-machine"
      },
      "env": {
        "PASSAGE_ENDPOINT": "https://passage.giantswarm.io",
        "API_ENDPOINT": "https://api.giantswarm.io"
      }
    }
  }
}
